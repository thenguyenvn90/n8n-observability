{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": false,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "type": "stat",
      "title": "n8n Up",
      "id": 2,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 0,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "min(up{job=\"$job\", instance=~\"$instance\"}) or vector(0)"
        }
      ],
      "pluginVersion": "12.0.0"
    },
    {
      "type": "stat",
      "title": "Uptime (seconds)",
      "id": 3,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 4,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "min(time() - (process_start_time_seconds{job=\"$job\", instance=~\"$instance\"} or n8n_process_start_time_seconds{job=\"$job\", instance=~\"$instance\"})) or vector(0)"
        }
      ],
      "pluginVersion": "12.0.0"
    },
    {
      "type": "stat",
      "title": "Queue Backlog (waiting)",
      "id": 4,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 10,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(n8n_scaling_mode_queue_jobs_waiting{job=\"$job\", instance=~\"$instance\"}) or sum({__name__=~\"n8n_queue_.*_waiting\", job=\"$job\", instance=~\"$instance\"}) or vector(0)"
        }
      ],
      "pluginVersion": "12.0.0"
    },
    {
      "type": "stat",
      "title": "Active Executions",
      "id": 5,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 16,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "none"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "n8n_active_executions{job=\"$job\", instance=~\"$instance\"} or vector(0)"
        }
      ],
      "pluginVersion": "12.0.0"
    },
    {
      "type": "stat",
      "title": "RSS Memory",
      "id": 6,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 4,
        "w": 4,
        "x": 20,
        "y": 0
      },
      "fieldConfig": {
        "defaults": {
          "unit": "bytes"
        },
        "overrides": []
      },
      "options": {
        "reduceOptions": {
          "calcs": [
            "lastNotNull"
          ],
          "fields": "",
          "values": false
        },
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "textMode": "auto"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "(n8n_process_resident_memory_bytes{job=\"$job\", instance=~\"$instance\"} or process_resident_memory_bytes{job=\"$job\", instance=~\"$instance\"})"
        }
      ],
      "pluginVersion": "12.0.0"
    },
    {
      "type": "timeseries",
      "title": "Queue throughput (completed / failed per second)",
      "id": 7,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 6,
        "w": 24,
        "x": 0,
        "y": 4
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none",
          "hideZeros": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (instance) (rate(n8n_scaling_mode_queue_jobs_completed{job=\"$job\", instance=~\"$instance\"}[5m])) or vector(0)",
          "legendFormat": "{{instance}} completed/s"
        },
        {
          "refId": "B",
          "expr": "sum by (instance) (rate(n8n_scaling_mode_queue_jobs_failed{job=\"$job\", instance=~\"$instance\"}[5m])) or vector(0)",
          "legendFormat": "{{instance}} failed/s"
        }
      ],
      "pluginVersion": "12.0.0"
    },
    {
      "type": "timeseries",
      "title": "Execution success ratio (5m)",
      "id": 8,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 5,
        "w": 12,
        "x": 0,
        "y": 10
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none",
          "hideZeros": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "100 * ( sum(rate(n8n_scaling_mode_queue_jobs_completed{job=\"$job\", instance=~\"$instance\"}[5m])) / clamp_min( sum(rate(n8n_scaling_mode_queue_jobs_completed{job=\"$job\", instance=~\"$instance\"}[5m])) + sum(rate(n8n_scaling_mode_queue_jobs_failed{job=\"$job\", instance=~\"$instance\"}[5m])), 1e-9) )",
          "legendFormat": "success %"
        }
      ],
      "pluginVersion": "12.0.0"
    },
    {
      "type": "timeseries",
      "title": "Event loop lag p99 (or gauge fallback)",
      "id": 9,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 5,
        "w": 12,
        "x": 12,
        "y": 10
      },
      "fieldConfig": {
        "defaults": {
          "unit": "s"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none",
          "hideZeros": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (le, instance) (rate(nodejs_eventloop_lag_seconds_bucket{job=\"$job\", instance=~\"$instance\"}[5m]))) or max by (instance) (n8n_nodejs_eventloop_lag_p99_seconds{job=\"$job\", instance=~\"$instance\"}) or max by (instance) (nodejs_eventloop_lag_seconds{job=\"$job\", instance=~\"$instance\"})",
          "legendFormat": "{{instance}} p99"
        }
      ],
      "pluginVersion": "12.0.0"
    },
    {
      "type": "timeseries",
      "title": "API error rate and ratio",
      "id": 10,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 5,
        "w": 24,
        "x": 0,
        "y": 15
      },
      "fieldConfig": {
        "defaults": {
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none",
          "hideZeros": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (instance) (rate(n8n_api_requests_failed_total{job=\"$job\", instance=~\"$instance\"}[5m])) or sum by (instance) (rate(n8n_api_requests_total{job=\"$job\", instance=~\"$instance\", code=~\"5..\"}[5m]))",
          "legendFormat": "{{instance}} 5xx/s"
        },
        {
          "refId": "B",
          "expr": "100 * ( sum(rate(n8n_api_requests_failed_total{job=\"$job\", instance=~\"$instance\"}[5m])) / clamp_min(sum(rate(n8n_api_requests_total{job=\"$job\", instance=~\"$instance\"}[5m])), 1e-9) )",
          "legendFormat": "{{instance}} error %"
        }
      ],
      "pluginVersion": "12.0.0"
    },
    {
      "type": "timeseries",
      "title": "CPU usage (cores)",
      "id": 11,
      "datasource": {
        "type": "prometheus",
        "uid": "${DS_PROM}"
      },
      "gridPos": {
        "h": 6,
        "w": 12,
        "x": 0,
        "y": 20
      },
      "fieldConfig": {
        "defaults": {
          "unit": "cores"
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true
        },
        "tooltip": {
          "mode": "single",
          "sort": "none",
          "hideZeros": false
        }
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (instance) (rate(n8n_process_cpu_seconds_total{job=\"$job\", instance=~\"$instance\"}[5m])) or sum by (instance) (rate(process_cpu_seconds_total{job=\"$job\", instance=~\"$instance\"}[5m]))",
          "legendFormat": "{{instance}} cores"
        }
      ],
      "pluginVersion": "12.0.0"
    }
  ],
  "preload": false,
  "refresh": "30s",
  "schemaVersion": 41,
  "style": "dark",
  "tags": [
    "n8n",
    "queue",
    "prometheus"
  ],
  "templating": {
    "list": [
      {
        "name": "job",
        "type": "query",
        "label": "n8n job",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROM}"
        },
        "query": "label_values({__name__=~\"n8n_.*\"}, job)",
        "refresh": 1,
        "includeAll": false,
        "multi": false,
        "current": {
          "text": "n8n",
          "value": "n8n"
        }
      },
      {
        "name": "instance",
        "type": "query",
        "label": "Instance",
        "datasource": {
          "type": "prometheus",
          "uid": "${DS_PROM}"
        },
        "query": "label_values(up{job=\"$job\"}, instance)",
        "refresh": 2,
        "includeAll": true,
        "multi": true
      }
    ]
  },
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "title": "n8n \u2013 Queue Mode (Mandatory Health)",
  "uid": "n8n-queue-mandatory-001",
  "version": 1,
  "__inputs": [
    {
      "name": "DS_PROM",
      "label": "Prometheus",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    }
  ],
  "__requires": [
    {
      "type": "grafana",
      "id": "grafana",
      "name": "Grafana",
      "version": "12.0.0"
    },
    {
      "type": "panel",
      "id": "timeseries",
      "name": "Time series",
      "version": "8.0.0"
    },
    {
      "type": "panel",
      "id": "stat",
      "name": "Stat",
      "version": "8.0.0"
    },
    {
      "type": "panel",
      "id": "barchart",
      "name": "Bar chart",
      "version": "8.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "2.0.0"
    }
  ]
}
